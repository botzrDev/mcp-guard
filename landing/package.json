{
  "name": "landing",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --open",
    "dev:host": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "watch": "ng build --watch --configuration development",
    "serve": "npx serve dist/landing/browser -l 4200",
    "preview": "npm run build && npm run serve",
    "clean": "rm -rf dist .angular",
    "rebuild": "npm run clean && npm run build",
    "test": "ng test",
    "lint": "ng lint",
    "format": "npx prettier --write \"src/**/*.{ts,html,scss}\""
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@types/gsap": "^1.20.2",
    "gsap": "^3.14.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "^21.0.0",
    "@playwright/test": "^1.57.0",
    "typescript": "~5.9.2"
  }
}
